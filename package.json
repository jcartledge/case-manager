{
  "name": "@jcartledge/case-manager",
  "version": "1.0.7",
  "repository": "github:jcartledge/case-manager",
  "description": "Simple conditional component for React.",
  "homepage": "https://jcartledge.github.io/case-manager/",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "James Cartledge <jcartledge@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@types/node": "20.6.4",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "ava": "5.3.1",
    "c8": "8.0.1",
    "eslint": "8.50.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
    "typedoc": "0.25.1",
    "typescript": "5.2.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "root": true
  },
  "lint-staged": {
    "*.ts": "lint"
  },
  "pnpm": {
    "auditConfig": {
      "ignoreCves": []
    },
    "overrides": {
      "optionator": ">=0.9.3",
      "semver@<7.5.2": ">=7.5.2"
    }
  },
  "scripts": {
    "build": "pnpm clean && tsc",
    "clean": "rimraf dist",
    "coverage": "c8 ava",
    "build-docs": "typedoc --out docs src",
    "lint": "eslint src && tsc --noEmit",
    "prepare": "husky install",
    "test": "ava"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "packageManager": "pnpm@8.7.6",
  "volta": {
    "node": "19.9.0"
  }
}
