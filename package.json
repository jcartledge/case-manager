{
  "name": "@jcartledge/case-manager",
  "version": "1.4.0",
  "repository": "github:jcartledge/case-manager",
  "description": "Simple conditional component for React.",
  "homepage": "https://jcartledge.github.io/case-manager/",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "James Cartledge <jcartledge@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.0",
    "@types/node": "20.15.0",
    "@typescript-eslint/eslint-plugin": "8.1.0",
    "@typescript-eslint/parser": "8.1.0",
    "ava": "6.1.3",
    "c8": "10.1.2",
    "eslint": "9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "9.1.4",
    "lint-staged": "15.2.9",
    "prettier": "^3.3.3",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typedoc": "0.26.5",
    "typescript": "5.5.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.ts": "pnpm lint:fix"
  },
  "pnpm": {
    "auditConfig": {
      "ignoreCves": []
    },
    "overrides": {
      "optionator": ">=0.9.3",
      "semver@<7.5.2": ">=7.5.2"
    }
  },
  "scripts": {
    "build": "pnpm clean && tsc",
    "clean": "rimraf dist",
    "coverage": "c8 --src=src --exclude=**/types --all --100 ava",
    "build-docs": "typedoc --out docs src",
    "lint": "eslint src && prettier src --check && tsc --noEmit",
    "lint:fix": "eslint src --fix && prettier src --write && tsc --noEmit",
    "prepare": "husky",
    "test": "ava --watch"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "packageManager": "pnpm@9.7.1",
  "volta": {
    "node": "20.16.0"
  }
}
