{
  "name": "@jcartledge/case-manager",
  "version": "1.4.0",
  "repository": "github:jcartledge/case-manager",
  "description": "Simple conditional component for React.",
  "homepage": "https://jcartledge.github.io/case-manager/",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "James Cartledge <jcartledge@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.14.0",
    "@types/node": "22.10.2",
    "@typescript-eslint/eslint-plugin": "8.18.2",
    "@typescript-eslint/parser": "8.18.2",
    "ava": "6.2.0",
    "c8": "10.1.3",
    "eslint": "9.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "9.1.7",
    "lint-staged": "15.3.0",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typedoc": "0.27.6",
    "typescript": "5.7.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.ts": "pnpm lint:fix"
  },
  "pnpm": {
    "auditConfig": {
      "ignoreCves": []
    },
    "overrides": {
      "micromatch@<4.0.8": ">=4.0.8",
      "cross-spawn@>=7.0.0 <7.0.5": ">=7.0.5",
      "@eslint/plugin-kit@<0.2.3": ">=0.2.3"
    }
  },
  "scripts": {
    "build": "pnpm clean && tsc",
    "clean": "rimraf dist",
    "coverage": "c8 --src=src --exclude=**/types --all --100 ava",
    "build-docs": "typedoc --out docs src",
    "lint": "eslint src && prettier src --check && tsc --noEmit",
    "lint:fix": "eslint src --fix && prettier src --write && tsc --noEmit",
    "prepare": "husky",
    "test": "ava --watch"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "packageManager": "pnpm@9.15.2",
  "volta": {
    "node": "22.12.0"
  }
}
