{
  "name": "@jcartledge/case-manager",
  "version": "1.0.6",
  "repository": "github:jcartledge/case-manager",
  "description": "Simple conditional component for React.",
  "homepage": "https://jcartledge.github.io/case-manager/",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "James Cartledge <jcartledge@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "ava": "^5.1.0",
    "c8": "^7.12.0",
    "eslint": "^8.32.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "rimraf": "^4.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "root": true
  },
  "lint-staged": {
    "*.ts": "lint"
  },
  "pnpm": {
    "auditConfig": {
      "ignoreCves": []
    }
  },
  "scripts": {
    "build": "pnpm clean && tsc",
    "clean": "rimraf dist",
    "coverage": "c8 ava",
    "build-docs": "typedoc --out docs src",
    "lint": "eslint src && tsc --noEmit",
    "prepare": "husky install",
    "test": "ava"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "packageManager": "pnpm@7.26.3",
  "volta": {
    "node": "19.5.0"
  }
}
